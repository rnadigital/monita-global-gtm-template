___TESTS___

scenarios:
- name: Test inject correct script
  code: |-
    const mockData = {
      "token": "template_test_script_NO_DELETE"
    };

    // Call runCode to run the template's code.
    runCode(mockData);

    // Verify that the tag finished successfully.
    assertApi('injectScript').wasNotCalledWith("https://cdn-a.raptor.digital/custom-config/template_test_script_NO_DELETE.js");
- name: Test custom functions
  code: |+
    const mockData = {
      "token": "template_test_script_NO_DELETE",
      "customFunctions": {
        custom_function: function() {/* Do nothing */ }
      }
    };

    // Call runCode to run the template's code.
    runCode(mockData);

    // Verify that the tag finished successfully.
    assertThat(theWindow.__monita_settings).isDefined();

    const functions = theWindow.__monita_settings;

    const functionNames = require("Object").keys(functions);

    assertThat(functionNames.length).isEqualTo(2);
    assertThat(functionNames.join(",")).contains("get_tms_data");
    assertThat(functionNames.join(",")).contains("custom_function");

- name: Test gtm and custom data
  code: |-
    const mockData = {
      "token": "template_test_script_NO_DELETE",
      "sessionId": "S123",
      "customerId": "C123"
    };

    // Call runCode to run the template's code.
    runCode(mockData);

    // Verify that the tag finished successfully.
    assertThat(theWindow.__monita_settings).isDefined();

    const customData = theWindow.__monita_settings.get_tms_data();

    assertThat(customData.rl).isEqualTo(1);
    assertThat(customData.env).isEqualTo("TST");
    assertThat(customData["do"]).isEqualTo("ABC");
    assertThat(customData.sid).isEqualTo("S123");
    assertThat(customData.cid).isEqualTo("C123");
setup: |-
  const containerVersion = {version: 1, environmentName: "TST", containerId: "ABC" };
  const theWindow = {};
  mock("getContainerVersion", function(){ return containerVersion; });
  mock("getFromWindow", function(key){ return theWindow[key]; });
  mock("setInWindow", function(key, val){ theWindow[key] = val; });


