/* Copyright RNA Digital PTY LTD */
// Name: Monita
const injectScript = require("injectScript");
const setInWindow = require("setInWindow");
const getContainerVersion = require("getContainerVersion");
const encodeUriComponent = require("encodeUriComponent");
const logToConsole = require("logToConsole");

logToConsole(data);

/**
 * USER CONFIG
 */
const url = "https://cdn-a.raptor.digital/custom-config/" + encodeUriComponent(data.token) + ".js";
const containerVersion = getContainerVersion();
const monitaSettings = data.customFunctions || {};
/**
 * STANDARD ACTIONS
 */
const gtmSettings = {
        get_tms_data: function(w) {
            // GTM Block
            const cv = containerVersion;
            if (cv) {
                const gtm = {
                    "s": "google_tag_manager", // System
                    "rl": cv.version, // Container Version
                    "env": cv.environmentName, // Container Environment Name
                    "do": cv.containerId, // Container ID
                    "et": 0,// Execution Time,
                    "vid": null, // ID
                    "cn": null, // Consent (provisioned for future release)
                    "sid": data.sessionId || "",// Session ID
                    "cid": data.customerId || "", // customer ID
                };
                return gtm;
            } else {
                return {};
            }
        }
    };

for (let i in gtmSettings) {
  monitaSettings[i] = gtmSettings[i];
}
setInWindow("__monita_settings", monitaSettings);
injectScript(url,
             function() {
              logToConsole("Success");
              data.gtmOnSuccess();
             },
             function(err) {
              logToConsole(err);
              data.gtmOnFailure();
             });


